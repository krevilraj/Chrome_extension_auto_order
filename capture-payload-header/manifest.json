{
  "manifest_version": 3,
  "name": "Network Capture DevTools",
  "version": "1.0",
  "description": "Adds a Network Capture panel to Chrome DevTools",
  "permissions": [
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html"

}