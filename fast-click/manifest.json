{
  "manifest_version": 3,
  "name": "Auto Click Send Button",
  "version": "1.0",
  "description": "Finds #send button and clicks it 5 times.",
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Click #send"
  }
}
